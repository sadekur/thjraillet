{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"layout": {
			"contentSize": "1183px",
			"wideSize": "1440px"
		},
		"color": {
			"palette": [
				{
					"color": "#131313",
					"name": "Black",
					"slug": "thraillet-black"
				},
				{
					"color": "#8B8B8B",
					"name": "Gray",
					"slug": "thraillet-grey"
				},
				{
					"color": "#f7f7f7",
					"name": "White",
					"slug": "thraillet-white"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Inter, sans-serif",
					"slug": "inter",
					"name": "Body - Inter",
					"fontFace": [
						{
							"fontFamily": "Inter, sans-serif",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/inter/Inter_24pt-Light.ttf"
							]
						},
						{
							"fontFamily": "Inter, sans-serif",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/inter/Inter_24pt-Regular.ttf"
							]
						},
						{
							"fontFamily": "Inter, sans-serif",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/inter/Inter_24pt-Bold.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Stolzl, Arial, sans-serif",
					"slug": "stolzl",
					"name": "Heading - Stolzl"
				}
			]
		},
		"custom": {
			"typography": {
				"font-size": {
					"heading-1": "clamp(2.113rem, 1.9252rem + 0.9390000000000001vw, 3.252rem)",
					"heading-2": "clamp(1.699rem, 1.5506000000000002rem + 0.7419999999999998vw, 2.441rem)",
					"heading-3": "clamp(1.35rem, 1.2294rem + 0.603vw, 1.953rem)",
					"heading-4": "clamp(1.253rem, 1.1909999999999998rem + 0.31000000000000005vw, 1.563rem)",
					"heading-5": "clamp(1.063rem, 1.0255999999999998rem + 0.18700000000000006vw, 1.25rem)",
					"heading-6": "clamp(1.063rem, 1.0255999999999998rem + 0.18700000000000006vw, 1.25rem)"
				}
			}
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"fontSize": "1rem",
			"lineHeight": "1.75"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--stolzl)",
					"fontWeight": "800",
					"lineHeight": "1.3",
					"fontSize": "var(--wp--custom--typography--font-size--heading-1)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--stolzl)",
					"fontWeight": "800",
					"lineHeight": "1.3",
