{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"layout": {
			"contentSize": "1183px",
			"wideSize": "1440px"
		},
		"color":{
			"palette": [
				{
					"color": "#131313",
					"name": "Black",
					"slug": "thraillet-black"
				},
				{
					"color": "#8B8B8B",
					"name": "Gray",
					"slug": "thraillet-grey"
				},
				{
					"color": "#f7f7f7",
					"name": "White",
					"slug": "thraillet-white"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Inter, sans-serif",
					"slug": "inter",
					"name": "Body - Inter",
					"fontFace": [{
						"fontFamily": "Inter, sans-serif",
						"fontWeight": "300",
						"fontStyle": "normal",
						"fontStretch": "normal",
						"src": ["file:./assets/fonts/inter/Inter_24pt-Light.ttf"]
					},
					{
						"fontFamily": "Inter, sans-serif",
						"fontWeight": "400",
						"fontStyle": "normal",
						"fontStretch": "normal",
						"src": ["file:./assets/fonts/inter/Inter_24pt-Regular.ttf"]
					},
					{
						"fontFamily": "Inter, sans-serif",
						"fontWeight": "600",
						"fontStyle": "normal",
						"fontStretch": "normal",
						"src": ["file:./assets/fonts/inter/Inter_24pt-Bold.ttf"]
					}]
				},
				{
					"fontFamily": "Stolzl, Arial, sans-serif",
					"slug": "stolzl",
					"name": "Heading - Stolzl"
				}
			]
		},
		"custom": {
			"typography": {
				"font-size":{
					"heading-1": "52px"
				}
			}
		}
		
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"fontSize": "1rem",
			"lineHeight": "1.75"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--stolzl)",
					"fontWeight": "800",
					"lineHeight": "1.3",
					"fontSize": "var(--wp--custom--typography--font-size--heading-1)"
				}
			}
		}
	}
}
