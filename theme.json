				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--stolzl)",
					"fontWeight": "800",
					"lineHeight": "1.3",
					"fontSize": "var(--wp--custom--typography--font-size--heading-6)"
				}
			}
		}
	}
}
